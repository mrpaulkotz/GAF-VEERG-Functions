--------------------------------------
*/

SourceData_Livestock_Common_FracWETMMS_Title
  =LAMBDA("Fraction of N available for leaching and runoff");

SourceData_Livestock_Common_FracWETMMS_Unit
  =LAMBDA("Fraction");

SourceData_Livestock_Common_FracWETMMS_Source
  =LAMBDA("National Inventory Report 2023 Volume 1: Equations 3.B.5a_3, 3.B.5c_3, 3.B.5d_3");

SourceData_Livestock_Common_FracWETMMS_Data
  =LAMBDA(1);

/* 
--------------------------------------
SourceData_Livestock_Common_FracLEACH_MS
FracLEACH_MS = 0.02 (Gg N/Gg applied) (CS EF, source IPCC (2019)) 
fraction of N lost through leaching and runoff
--------------------------------------
*/

SourceData_Livestock_Common_FracLEACH_MS_Title
  =LAMBDA("Fraction of N lost through leaching and runoff");

SourceData_Livestock_Common_FracLEACH_MS_Unit
  =LAMBDA("Fraction");

SourceData_Livestock_Common_FracLEACH_MS_Source
  =LAMBDA("National Inventory Report 2023 Volume 1: Equations 3.B.5a_3, 3.B.5c_3, 3.B.5d_3");

SourceData_Livestock_Common_FracLEACH_MS_Data
  =LAMBDA(
    MAKEARRAY(8, 2, 
      LAMBDA(r,c, 
        INDEX({
          "Production system", "Emission factor";
            "Irrigated pasture", 0.0059;
            "Irrigated crop", 0.007;
            "Non-irrigated pasture", 0.0018;
            "Non-irrigated crop", 0.0041;
            "Sugar cane", 0.0199;
            "Cotton", 0.0053;
            "Horticulture", 0.0064
        }, r, c)
      )
    )
  );


